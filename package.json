    "wechat-oauth",
    "wechat-payment"
  ],
  "dependencies": {
    "co-wechat": "^2.3.0",
    "co-wechat-api": "^3.8.1",
    "co-wechat-oauth": "^2.0.1",
    "co-wechat-payment": "^0.2.0",
    "egg-redis": "^2.1.0"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg": "^2.14.1",
    "egg-bin": "^4.9.0",
    "egg-ci": "^1.10.0",
    "egg-mock": "^3.20.1",
    "eslint": "^5.9.0",
    "eslint-config-egg": "^7.1.0",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "node": ">=8.9.0"
